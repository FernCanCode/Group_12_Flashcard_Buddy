<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.MainController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resources/Main%20Menu.png" />
               </image>
            </ImageView>
            <Button fx:id="make" layoutX="150.0" layoutY="200.0" mnemonicParsing="false" onAction="#make" prefHeight="30.0" prefWidth="300.0" text="MAKE" />
            <Button fx:id="study" layoutX="150.0" layoutY="240.0" mnemonicParsing="false" onAction="#study" prefHeight="30.0" prefWidth="300.0" style="-fx-border-color: transparent;" text="STUDY" />
            <Button fx:id="exit" layoutX="150.0" layoutY="280.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="300.0" style="-fx-border-color: transparent;" text="EXIT" />
         </children>
      </Pane>
   </children>
</AnchorPane>
